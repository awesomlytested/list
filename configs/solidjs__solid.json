{
    "repo": "solidjs/solid",
    "url": "https://github.com/solidjs/solid",
    "branch": "main",
    "configs": [
        {
            "package": "solid-js",
            "lang": "ts",
            "dir": "packages/solid/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "solid-js/store",
            "lang": "ts",
            "dir": "packages/solid/store/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "solid-js/web",
            "lang": "js",
            "dir": "packages/solid/web/test",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "test-integration",
            "lang": "ts",
            "dir": "packages/test-integration/tests",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        }
    ]
}