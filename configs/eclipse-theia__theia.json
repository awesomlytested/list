{
    "repo": "eclipse-theia/theia",
    "url": "https://github.com/eclipse-theia/theia",
    "branch": "master",
    "configs": [
        {
            "package": "@theia/electron",
            "lang": "js",
            "dir": "dev-packages/electron/tests",
