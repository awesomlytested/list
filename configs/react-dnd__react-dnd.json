            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "react-dnd-touch-backend",
            "lang": "ts",
            "dir": "packages/backend-touch/src/__tests__",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "dnd-core",
            "lang": "ts",
            "dir": "packages/dnd-core/src/__tests__",
