{
    "repo": "youzan/vant",
    "url": "https://github.com/youzan/vant",
    "branch": "dev",
    "configs": [
        {
            "package": "@vant/eslint-config",
            "lang": "js",
            "dir": "packages/vant-eslint-config/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@vant/use",
            "lang": "ts",
            "dir": "packages/vant-use/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "vant",
            "lang": "ts",
            "dir": "packages/vant/test",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        }
    ]
}