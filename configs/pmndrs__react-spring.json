{
    "repo": "pmndrs/react-spring",
    "url": "https://github.com/pmndrs/react-spring",
    "branch": "master",
    "configs": [
        {
            "package": "react-spring--root",
            "lang": "js",
            "dir": "cypress",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@react-spring/core",
            "lang": "ts",
            "dir": "packages/core/test",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "@react-spring/parallax",
            "lang": "ts",
            "dir": "packages/parallax/test",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        }
    ]
}