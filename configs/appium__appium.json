{
    "repo": "appium/appium",
    "url": "https://github.com/appium/appium",
    "branch": "master",
    "configs": [
        {
            "package": "appium",
            "lang": "js",
            "dir": "test",
            "framework": "mocha",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "javascript-wd",
            "lang": "js",
            "dir": "sample-code/javascript-wd/test",
            "framework": "mocha",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "webdriverio-sample-appium-tests",
            "lang": "js",
            "dir": "sample-code/javascript-webdriverio/test",
            "framework": "mocha",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        }
    ]
}