{
    "repo": "neoclide/coc.nvim",
    "url": "https://github.com/neoclide/coc.nvim",
    "branch": "master",
    "configs": [
        {
            "package": "coc.nvim-master",
            "lang": "ts",
            "dir": "src/__tests__",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "lang": "js",
            "dir": "src/__tests__/extensions/test",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        }
    ]
}