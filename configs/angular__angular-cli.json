{
    "repo": "angular/angular-cli",
    "url": "https://github.com/angular/angular-cli",
    "branch": "master",
    "configs": [
        {
            "package": "@angular/devkit-repo",
            "lang": "js",
            "dir": "tests",
            "framework": "jasmine",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@angular-devkit/build-angular",
            "lang": "js",
            "dir": "packages/angular_devkit/build_angular/test",
            "framework": "jasmine",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@angular-devkit/build-webpack",
            "lang": "js",
            "dir": "packages/angular_devkit/build_webpack/test",
            "framework": "jasmine",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        }
    ]
}