{
    "repo": "BabylonJS/Babylon.js",
    "url": "https://github.com/BabylonJS/Babylon.js",
    "branch": "master",
    "configs": [
        {
            "package": "babylonjs",
            "lang": "js",
            "dir": "tests",
            "framework": "mocha",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "babylonjs-viewer",
            "lang": "js",
            "dir": "Viewer/tests",
            "framework": "mocha",
            "pattern": "**/*.{js,ts}"
        }
    ]
}