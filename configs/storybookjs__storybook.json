{
    "repo": "storybookjs/storybook",
    "url": "https://github.com/storybookjs/storybook",
    "branch": "next",
    "configs": [
        {
            "package": "@storybook/addon-storyshots-puppeteer",
            "lang": "ts",
            "dir": "addons/storyshots/storyshots-puppeteer/src/__tests__",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "html-kitchen-sink",
            "lang": "js",
            "dir": "examples/html-kitchen-sink/tests",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "official-storybook",
            "lang": "js",
            "dir": "examples/official-storybook/tests",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@storybook/cli",
            "lang": "js",
            "dir": "lib/cli/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@storybook/ui",
            "lang": "js",
            "dir": "lib/ui/src/__tests__",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "@storybook/root",
            "lang": "ts",
            "dir": "cypress",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        }
    ]
}