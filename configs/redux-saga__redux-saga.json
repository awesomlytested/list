{
    "repo": "redux-saga/redux-saga",
    "url": "https://github.com/redux-saga/redux-saga",
    "branch": "master",
    "configs": [
        {
            "package": "babel-plugin-redux-saga",
            "lang": "js",
            "dir": "packages/babel-plugin-redux-saga/test",
            "framework": "jest",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        },
        {
            "package": "@redux-saga/core",
            "lang": "js",
            "dir": "packages/core/__tests__",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "@redux-saga/testing-utils",
            "lang": "js",
            "dir": "packages/testing-utils/__tests__",
            "framework": "jest",
            "pattern": "**/*.{js,ts}"
        }
    ]
}