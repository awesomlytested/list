{
    "repo": "enzymejs/enzyme",
    "url": "https://github.com/enzymejs/enzyme",
    "branch": "master",
    "configs": [
        {
            "package": "enzyme",
            "lang": "js",
            "dir": "test",
            "framework": "mocha",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "enzyme-example-mocha",
            "lang": "js",
            "dir": "packages/enzyme-example-mocha/test",
            "framework": "mocha",
            "pattern": "**/*.{js,ts}"
        },
        {
            "package": "enzyme-test-suite",
            "lang": "js",
            "dir": "packages/enzyme-test-suite/test",
            "framework": "mocha",
            "pattern": "**/*[._-]{test,spec,unittest,unit}.{ts,js}"
        }
    ]
}